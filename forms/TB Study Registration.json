      "rule" : "'use strict';\n({params, imports}) => {\n  const individual = params.entity;\n  const moment = imports.moment;\n  const formElement = params.formElement;\n  const _ = imports.lodash;\n  let visibility = true;\n  let value = null;\n  let answersToSkip = [];\n  let validationErrors = [];\n  \n  const condition11 = new imports.rulesConfig.RuleCondition({individual, formElement}).when.valueInRegistration(\"8e6c84cc-7a7e-4b89-8748-1ed44de7a9f7\").containsAnswerConceptName(\"669536f4-a675-402b-b3e4-eb7b553c90ce\").matches();\n    \n  const condition12 = new imports.rulesConfig.RuleCondition({individual, formElement}).when.valueInRegistration(\"8e6c84cc-7a7e-4b89-8748-1ed44de7a9f7\").containsAnswerConceptName(\"19a5d302-7921-4111-bb29-e443f78eabc4\").matches();\n  \n  visibility = condition11 ;\n  if(condition11 ){\n    value = \"Kota\";  \n}\n  if(condition11 ) {\n    _.forEach([\"f8ee26ef-f40f-4d26-ae76-ae5d7e0c3f81\"], (answer) => {\n        const answerToSkip = formElement.getAnswerWithConceptUuid(answer);\n        if (answerToSkip) answersToSkip.push(answerToSkip);\n    });\n};\n    if(condition12 ){\n    value = \"Lormi\";  \n}\n  \n  return new imports.rulesConfig.FormElementStatus(formElement.uuid, visibility, value, answersToSkip, validationErrors);\n};",
