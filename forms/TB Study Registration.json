  "visitScheduleRule" : "\"use strict\";\n({ params, imports }) => {\n  const individual = params.entity;\n  const moment = imports.moment;\n  const scheduleBuilder = new imports.rulesConfig.VisitScheduleBuilder({individual});\n  \n  const condition11 = new imports.rulesConfig.RuleCondition({individual}).when.lowestAddressLevel.defined.matches();\n  \n  if(condition11 ){\n    const earliestDate = moment(individual.registrationDate).add(0, 'days').toDate();\n    const maxDate = moment(individual.registrationDate).add(0, 'days').toDate();\n    scheduleBuilder.add({name: \"TB Study Baseline History \", encounterType: \"TB Study Baseline History \", earliestDate, maxDate});  \n}\n  \n  return scheduleBuilder.getAll();\n};",
